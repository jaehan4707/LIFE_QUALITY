                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/custom_radiobutton"
                    android:button="@null"
                    android:layout_marginTop="20dp"/>
                <RadioButton
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/custom_radiobutton"
                    android:button="@null"
                    android:layout_marginTop="20dp"/>
                <RadioButton
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/custom_radiobutton"
                    android:button="@null"
                    android:layout_marginTop="20dp"/>
